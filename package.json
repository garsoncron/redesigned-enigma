{
  "name": "myapp-monorepo",
  "private": true,
  "packageManager": "pnpm@3.3.1",
  "prisma": {
    "schema": "./packages/db/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "turbo run dev",
    "web": "turbo run dev --filter nextjs",
    "native": "cd apps/expo && pnpm dev",
    "desktop": "turbo run dev:tauri --filter nextjs",
    "build": "turbo build",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean ; rm -rf .pnpm/cache .pnpm/install-state.gz",
    "db-push": "turbo db-push"
  },
  "resolutions": {
    "esbuild": "0.15.5",
    "@types/react": "17.0.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "0.18.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.19.1",
    "@types/react": "^18.2.14",
    "@types/react-native": "^0.72.2",
    "eslint": "^8.21.0",
    "lerna-update-wizard": "^1.1.2",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "turbo": "^1.4.2",
    "typescript": "^4.9.4"
  }
}
